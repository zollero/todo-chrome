{
	"name": "Zero(Beta)",
	"version": "0.0.2",
	"manifest_version": 2,
	"description": "An extension to manage todo list.",
	"icons": {
    "16": "./static/icons/icon-16.png",
    "48": "./static/icons/icon-48.png",
    "128": "./static/icons/icon-128.png"
  },
	"browser_action": {
		"default_icon": {
      "19": "./static/icons/icon-19.png",
      "38": "./static/icons/icon-38.png"
    },
		"default_title": "Zero(Beta)",
		"default_popup": "./index.html"
	},
	"permissions": [
    "tabs",
    "activeTab",
    "notifications",
    "storage",
    "unlimitedStorage"
  ],
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
	// "options_page": "options.html",
	"background": {
		"scripts": ["./background.js"]
	}
}
